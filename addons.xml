<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="repository.finnish-tv" name="Finnish TV" version="1.0.2" provider-name="szymex">
	<extension point="xbmc.addon.repository" name="Finnish TV">
		<info compressed="false">https://raw.githubusercontent.com/szymex/xbmc-finnish-tv/master/addons.xml</info>
		<checksum>https://raw.githubusercontent.com/szymex/xbmc-finnish-tv/master/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.githubusercontent.com/szymex/xbmc-finnish-tv/master/repo/</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Add-ons for finnish tv</summary>
		<description>Download and install addons from finnish tv repository.</description>
		<platform>all</platform>
	</extension>
</addon>
<addon id="plugin.video.katsomo"
       name="Katsomo"
       version="1.2.3"
       provider-name="szymex">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
    <import addon="script.module.parsedom" version="0.9.1"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="katsomo.py">
        <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary>Katsomo</summary>
	<language>fi</language>
    <description>TV Videos from Katsomo.fi (mtv3, sub, ava)</description>
    <platform>all</platform>
	<nofanart>false</nofanart>
  </extension>
</addon>
